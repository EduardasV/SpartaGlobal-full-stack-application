<h1><%= @movie.title %></h1>
<p><%= @movie.movie_description %></p>
<img src="<%= @movie.movie_image %>" width='225' height="315">
<iframe width="560" height="315" src="<%= @movie.movie_trailer %>" frameborder="0" allow="autoplay; encrypted-media" allowfullscreen></iframe>
<form action="<%= @movie.id %>" method="post">
  <input type="hidden" name="_method" value="DELETE">
  <input type="submit" value="Delete">
</form>
<a href="/<%= @movie.id%>/edit"><button>Edit</button></a>
